    "test": "c8 ava",
    "test:watch": "ava --watch",
    "format": "prettier --write . "
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.4",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@types/node": "^18.11.18",
    "ava": "^5.1.0",
    "c8": "^7.12.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
