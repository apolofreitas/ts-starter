{
  "name": "ts-starter",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node build/main.js",
    "build": "swc src -d build --copy-files --delete-dir-on-start",
    "type-check": "tsc --noEmit",
