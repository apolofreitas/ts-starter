{
  "name": "ts-starter",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prod": "node build/main.js",
    "build": "vite build",
    "start": "vite-node src/main.ts",
    "dev": "vite-node --watch src/main.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest --watch",
    "test:run": "vitest --run --coverage",
    "format": "prettier --write . "
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@vitest/coverage-c8": "^0.29.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-node": "^0.29.2",
    "vitest": "^0.29.2"
  },
  "engines": {
    "node": "~18"
  }
}
